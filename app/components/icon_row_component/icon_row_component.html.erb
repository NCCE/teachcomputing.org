
<div class="icon-row">
  <% @icons.each do |icon| %>
    <div class="icon-row_icon">

      <%= image_pack_tag icon[:image_url] if icon[:image_url] %>
      <div class="icon-row_icon_content">
        <p class="govuk-body govuk-!-text-align-centre"><%= icon[:text] %></p>
      </div>

    </div>
  <% end %>
</div>