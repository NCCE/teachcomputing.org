
<script>
document.addEventListener('DOMContentLoaded', function() {
  const endpoint = "https://preprod-account.stem.org.uk/user/logout"
  const endpoint_alt = "https://www-preprod.stem.org.uk/user/logout"

  fetch(endpoint, {
    method: 'GET', 
  })
  .then(response => {
    if (!response.ok) {
      throw new Error('Network response was not ok ' + response.statusText);
    }
    console.log(response)
  })
  .catch(error => {
    console.error('There has been a problem with your fetch operation:', error);
  });

  fetch(endpoint_alt, {
    method: 'GET', 
  })
  .then(response => {
    if (!response.ok) {
      throw new Error('Network response was not ok ' + response.statusText);
    }
    console.log(response)
  })
  .catch(error => {
    console.error('There has been a problem with your fetch operation:', error);
  });


});

</script>