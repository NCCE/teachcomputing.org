<ul class="govuk-list ncce-activity-list ncce-activity-list--programme" role="list">
  <li class="activity-list__item <%= 'activity-list__item--incomplete' unless @programme.user_completed_diagnostic?(current_user) %>">
    Take our quiz to assess your subject knowledge and identify areas for development<br>
		<% unless @programme.user_completed_diagnostic?(current_user) %>
      <%= link_to 'Take the quiz', cs_accelerator_diagnostic_path(Activity.cs_accelerator_diagnostic_tool.id), class: 'govuk-button button button--small govuk-!-margin-top-1' %>
    <% end %>
  </li>
  <%= render 'programmes/cs-accelerator/10_hours/achievements' %>
  <%= render 'programmes/cs-accelerator/10_hours/exam' %>
</ul>
