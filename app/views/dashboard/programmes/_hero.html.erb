<% if programme.user_enrolled?(current_user) %>
  <% if programme.user_completed?(current_user) %>
    <%= render "dashboard/programmes/complete", programme: programme %>
  <% else%>
    <%= render "dashboard/programmes/progress", programme: programme %>
  <% end %>
<% else %>
  <%= render "dashboard/programmes/enrol", programme: programme %>
<% end %>
