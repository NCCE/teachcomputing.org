<% programme = Programme.cs_accelerator %>
<div class="govuk-grid-column-one-third">
	<div class="shadow_card card--dashboard card--dashboard--cs-accelerator %>">
			<%= render 'dashboard/programme-enrolment-status', programme: programme %>
			<div class="card--dashboard--content">
				<h3 class="govuk-heading-m card__heading">
					<%= link_to 'Teach GCSE computer science', cs_accelerator_path, class: 'ncce-link ncce-link--heading' %>
				</h3>
				<ul class="govuk-list govuk-list--bullet govuk-!-margin-top-4">
						<li>blended course and activity programme</li>
						<li>flexible and tailored to your needs</li>
						<li>bursaries for eligible schools</li>
				</ul>
				<%= render 'dashboard/enrolment-status', programme: programme %>
			</div>
		</div>
</div>
