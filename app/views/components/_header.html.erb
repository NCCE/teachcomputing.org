    <button type="button" class="govuk-header__menu-button govuk-js-header-toggle" aria-controls="navigation" aria-label="Show or hide Top Level Navigation" hidden>Menu</button>
      <% if current_user %>
        <%= render NotificationComponent.new(user: current_user, wrapper_css: 'tablet-notifications govuk-header__link') %>
      <% end %>
      <div id="navigation" class="govuk-header__navigation ncce-header__navigation" aria-label="Top Level Navigation">
        <% if current_user %>
          <ul class='ncce-header__user'>
            <li class="govuk-header__navigation-item ncce-header__navigation-item">
              <%= render NotificationComponent.new(user: current_user, wrapper_css: 'desktop-notifications govuk-header__link') %>
            </li>
            <li class="govuk-header__navigation-item ncce-header__navigation-item">
              <%= link_to 'Your dashboard', dashboard_path, class: 'govuk-header__link', data: { event_action: 'click', event_category: 'Header: Navigation', event_label: 'Your dashboard' } %>
            </li>
            <li class="govuk-header__navigation-item ncce-header__navigation-item">
              <%= link_to 'Edit profile', profile_edit_url, class: 'govuk-header__link', data: { event_action: 'click', event_category: 'Header: Navigation', event_label: 'Edit profile' } %>
            </li>
            <li class="govuk-header__navigation-item ncce-header__navigation-item">
              <%= link_to 'Logout', logout_url, class: 'govuk-header__link', data: { event_action: 'click', event_category: 'Header: Navigation', event_label: 'Logout' } %>
            </li>
          </ul>
        <% else %>
          <ul class="ncce-header__user">
            <li class="govuk-header__navigation-item ncce-header__navigation-item">
              <%= link_to 'Create an account', "#{auth_url}?screen_hint=signup", method: :post, :class => 'govuk-header__link', data: { event_action: 'click', event_category: 'Header: Navigation', event_label: 'Create an account' } %>
            </li>
            <li class="govuk-header__navigation-item ncce-header__navigation-item">
              <%= link_to 'Log in', auth_url, method: :post, :class => 'govuk-header__link', data: { event_action: 'click', event_category: 'Header: Navigation', event_label: 'Log in' } %>
            </li>
          </ul>
        <% end %>
        <div class="ncce-header__site-search">
          <%= render SiteSearchComponent.new %>
        </div>
        <ul class='ncce-header__certification'>
          <% header_navigation.each do | nav_item |%>
            <li class="govuk-body  ncce-header__certification-item dropdown__expander" aria-expanded="false" tabindex="0">
              <div class="ncce-header__wrap">
                <span class="ncce-header__certification-item--text"><%= nav_item[:text] %></span>
                <span class="ncce-header__certification-item--icon"></span>
              </div>
              <ul class="govuk-list govuk-body-s dropdown__expander-content">
              <% nav_item[:children].each do | item |%>
                <li class="dropdown__expander-content-item"><%= link_to item[:text], item[:link], data: { event_action: 'click', event_category: 'Header: Navigation', event_label: item[:label] }, class: ' ncce-link--on-dark' %></li>
              <% end %>
              </ul>
            </li>
          <% end %>
        </ul>
      </div>
    </nav>
  </div>
