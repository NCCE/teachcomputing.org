extends:
  - 'eslint:recommended'
  - prettier
parser: babel-eslint
plugins:
  - prettier
parserOptions:
  sourceType: module
  ecmaVersion: 2018
  ecmaFeatures:
    experimentalObjectRestSpread: true
env:
  browser: true
  es6: true
  node: true
rules:
  no-console: 0
  prettier/prettier:
    - error